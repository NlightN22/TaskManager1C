<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="OwnButtonDisabled">
        <item name="android:background">@drawable/ic_button_detailed</item>
        <item name="android:textColor">@color/edit_text_background</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:enabled">false</item>
        <item name="android:paddingStart">12dp</item>
        <item name="android:paddingEnd">12dp</item>
        <item name="android:paddingTop">16dp</item>
    </style>
    <style name="OwnButtonHintDisabled">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/edit_text_top_hint</item>
        <item name="android:textSize">12sp</item>
        <item name="android:layout_marginTop">-3dp</item>
    </style>
    <style name="ButtonNotEditableMaterial">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:backgroundTint">@color/text_item_background_not_editable</item>
        <item name="cornerRadius">16dp</item>
        <item name="android:enabled">false</item>
        <item name="android:paddingTop">16dp</item>
        <item name="android:paddingStart">30dp</item>
        <item name="android:paddingEnd">30dp</item>
    </style>
</resources>