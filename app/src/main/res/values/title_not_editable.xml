<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="TitleNotEditableCardView" parent="CardViewGlobal">
    </style>
    <style name="TitleTopperText">
        <item name="backgroundTint">@color/text_item_background_not_editable</item>
        <item name="android:textColor">@color/edit_text_top_hint</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textStyle">normal</item>
        <item name="android:ellipsize">end</item>
        <item name="android:maxLines">1</item>
        <item name="android:maxEms">7</item>
    </style>
    <style name="TitleText" parent="TitleTopperText">
        <item name="android:textSize">20sp</item>
        <item name="android:gravity">center</item>
        <item name="android:textStyle">bold</item>
        <item name="android:maxEms">20</item>
        <item name="android:maxLines">3</item>
    </style>



</resources>